{
	"name": "n8n-nodes-abyssale",
	"version": "1.0.1",
	"description": "Transform one design into hundreds of on-brand assets across all marketing channels.",
	"license": "MIT",
	"homepage": "https://www.abyssale.com/",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "remyAbyssale",
		"email": "remy@abyssale.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/getabyssale/n8n-nodes-abyssale.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
